{
  "canisters": {
    "vault_backend": {
      "candid": "src/vault_backend/vault_backend.did",
      "package": "vault_backend",
      "type": "rust"
    },
    "vault_frontend": {
      "dependencies": [
        "vault_backend"
      ],
      "source": [
        "src/vault_frontend/dist"
      ],
      "type": "assets",
      "workspace": "vault_frontend"
    }, 
    "xrc": {
      "wasm": "src/xrc/xrc.wasm",
      "type": "custom", 
      "candid": "src/xrc/xrc.did"
    }, 
    "ledger": {
      "wasm": "src/ledger/ic-icrc1-ledger.wasm.gz",
      "candid": "src/ledger/ledger.did",
      "type": "custom", 
      "shrink": false
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}